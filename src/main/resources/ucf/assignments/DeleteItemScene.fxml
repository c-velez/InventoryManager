<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="4.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.DeleteItemController">
   <children>
      <Label text="Are you sure you would like to remove this item?" />
      <HBox alignment="CENTER" spacing="4.0">
         <children>
            <Button fx:id="yesButton" mnemonicParsing="false" onAction="#yesButtonPressed" text="Yes" />
            <Button fx:id="noButton" mnemonicParsing="false" onAction="#noButtonPressed" text="No" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
